{
	"extends": ["next/core-web-vitals", "plugin:storybook/recommended", "prettier"],
	"settings": {
		"next": {
			"rootDir": "frontend/"
		}
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-explicit-any": "warn",
		"@typescript-eslint/no-unused-vars": "warn",
		"semi": ["warn", "never"]
	}
}
